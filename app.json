{
  "expo": {
    "name": "FocusTime",
    "slug": "FocusTime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/tomato.png",
    "splash": {
      "image": "./images/tomato.png",
      "resizeMode": "contain",
      "backgroundColor": "wheat"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "web": {
      "favicon": "./images/favicon.png"
    }
  }
}
